syntax = "proto2";

package lcs.srv;

import "lcs_common.proto";

message Session
{
    required string mid             = 1;
    required ServerStatus status    = 2;
    optional Channel channel        = 3;
}

message Sessions
{
    repeated Session sessions       = 1;
}

message SessionResp
{
    enum ErrorCode
    {
        kOK = 0; //stored successfully
        kError = 1; //
    }

    required ErrorCode ec = 1;
}

